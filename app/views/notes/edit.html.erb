<h1>New Note</h1>
<p class="bg-primary">Edit your Note!</p>

<%= form_for(@note) do |f| %>
  
  <h3><%= f.label :date %></h3>
  <%= f.date_select :date %>

  <h3><%= f.label :title %></h3>
  <%= f.text_field :title, class: "form-control"%>

  <h3><%= f.label :body %></h3>
  <%= f.text_area :body %>

  <br>
  <%= f.submit "更新" %>
<% end %>
