<h1>New Profile</h1>
<p class="bg-warning">Edit Page</p>
<%= form_for(@profile) do |f| %>
  
  <h3><%= f.label :name %></h3>
  <%= f.text_field :name, class: "form-control" %>

  <h3><%= f.label :avatar %></h3>
  <%= f.text_field :avatar, class: "form-control"%>

  <br>
  <%= f.submit "更新" %>
<% end %>
